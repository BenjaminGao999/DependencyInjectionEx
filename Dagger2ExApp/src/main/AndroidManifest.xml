    <application
        android:name="androidsimple.gaos.dependencyinjectionex.scope.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="androidsimple.gaos.dependencyinjectionex.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="androidsimple.gaos.dependencyinjectionex.MainActivity2"
            android:label="Login" />
        <activity
            android:name="androidsimple.gaos.dependencyinjectionex.scope.a.AActivity"
            android:label="A" />
        <activity
            android:name="androidsimple.gaos.dependencyinjectionex.scope.b.BActivity"
            android:label="B" />
        <activity
            android:name="androidsimple.gaos.dependencyinjectionex.scope.c.CActivity"
            android:label="C" />
    </application>

</manifest>